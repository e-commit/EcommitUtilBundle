{
    "name": "ecommit/util-bundle",
    "description": "EcommitUtilBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "require": {
        "doctrine/annotations": "*",
        "doctrine/inflector": "*",
        "symfony/config": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/property-info": "^4.0",
        "twig/twig": "~1.34|~2.4"
    },
    "suggest": {
        "doctrine/orm": "Required if ClearEntityManager or GenerateEntitiesCommand is used",
        "guzzlehttp/guzzle": "Required if clear APCU is enabled in upgrade command",
        "symfony/doctrine-bridge": "Required if GenerateEntitiesCommand is used",
        "symfony/http-kernel": "Required if CompareLocalesCommand is used",
        "symfony/translation": "Required if CompareLocalesCommand is used",
        "zendframework/zend-cache": "Required if Cache is used"
    },
    "autoload": {
        "psr-4": { "Ecommit\\UtilBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5.x-dev"
        }
    }
}
