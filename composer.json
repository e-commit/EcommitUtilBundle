{
    "name": "ecommit/util-bundle",
    "description": "EcommitUtilBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.8|~3.0",
        "twig/twig": "*"
    },
    "suggest": {
        "zendframework/zend-cache": "Required if cache is used",
        "doctrine/orm": "Required if ClearEntityManager or GenerateEntitiesCommand is used"
    },
    "autoload": {
        "psr-0": { "Ecommit\\UtilBundle": "" }
    },
    "target-dir": "Ecommit/UtilBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
